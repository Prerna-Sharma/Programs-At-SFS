<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_empty"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.student.androidprograms.com.example.student.parcelabledatafiles.ShowParcelableDataActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_setting"
        android:id="@+id/name_textview"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/margin_setting"
        android:layout_height="wrap_content"
        android:id="@+id/age_textview"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/margin_setting"
        android:layout_height="wrap_content"
        android:id="@+id/course_textview"/>

</LinearLayout>
